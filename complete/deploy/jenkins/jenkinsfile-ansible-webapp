node {
  stage('Init') {
    checkout scm
  }

  stage('Build') {
    sh '''
        cd complete
        mvn clean package
        mv target/*.war ROOT.war
    '''
  }

  stage('Deploy') {

    azureWebAppPublish azureCredentialsId: "542d982a-7614-4a3d-ae6b-ce321bfef5b0",
                       resourceGroup: "hiepit07", 
                       appName: "hiepit07", 
                       filePath: "ROOT.war"
  }
}
